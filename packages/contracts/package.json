{
    "name": "contracts",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "compile": "hardhat compile",
        "coverage": "hardhat coverage",
        "test": "hardhat test",
        "gas-report": "env ENABLE_GAS_REPORT=1 yarn test",
        "run-localchain": "yarn hardhat node",
        "deploy:0": "yarn hardhat run scripts/0_deployTemplate.ts",
        "deploy:1": "yarn hardhat run scripts/1_deployFactory.ts",
        "deploy-contract-to-localchain": "hardhat run scripts/99_batchMigration.ts --network localhost",
        "deploy-contract-to-rinkeby": "hardhat run scripts/99_batchMigration.ts --network rinkeby",
        "deploy-contract-to-matic_test": "hardhat run scripts/99_batchMigration.ts --network matic_test",
        "deploy-contract-to-bsc_test": "hardhat run scripts/99_batchMigration.ts --network bsc_test"
    },
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.0.1",
        "@nomiclabs/hardhat-etherscan": "^2.1.1",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@typechain/ethers-v5": "^5.0.0",
        "@types/mocha": "^8.2.0",
        "chai": "^4.3.1",
        "ethereum-waffle": "^3.2.2",
        "ethers": "^5.0.31",
        "hardhat": "^2.0.10",
        "hardhat-gas-reporter": "^1.0.4",
        "hardhat-typechain": "^0.3.4",
        "solidity-coverage": "^0.7.15",
        "typechain": "^4.0.1"
    },
    "dependencies": {
        "@openzeppelin/contracts": "4.0.0-rc.0",
        "@openzeppelin/contracts-upgradeable": "4.0.0-rc.0"
    }
}
